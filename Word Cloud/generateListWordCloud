import random


def create_words():
    possible_words = ["Daft Punk", "Eric Andre", "Kraft Punk", "Aphex Twin", "Charlie Brown", "Bill Watterson"]
    p = random.randrange(-0.5, 0.5, .1)
    weights = [30 + p, 10 + p, 5 + p, 20 + p, 15 + p, 20 + p]

    word_list = random.choices(population=possible_words, weights=weights, k=100000)

    return word_list
